  "bugs": {
    "url": "https://github.com/connor-stokes/Courier/issues"
  },
  "homepage": "https://github.com/connor-stokes/Courier#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/supertest": "^2.0.9",
    "jest": "^26.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/helmet": "^0.0.48",
    "@types/cors": "^2.8.7",
